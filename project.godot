config/features=PackedStringArray("4.1", "C#", "GL Compatibility")
run/max_fps=30
boot_splash/bg_color=Color(0.270588, 0.156863, 0.235294, 1)
boot_splash/image="res://Icons/icon_256.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://Icons/icon_256.png"
config/windows_native_icon="res://Icons/icon_256.ico"

[display]

window/size/viewport_width=500
window/size/viewport_height=500
window/energy_saving/keep_screen_on=false

[dotnet]

project/assembly_name="Zero Sievert GNR"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
